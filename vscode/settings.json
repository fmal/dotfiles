{
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "poimandres",
  "workbench.iconTheme": "Monokai Pro Icons",
  "workbench.colorCustomizations": {
    "[Monokai Pro]": {
      "titleBar.activeBackground": "#221f22",
      "editorGroupHeader.tabsBackground": "#221f22",
      "tab.inactiveBackground": "#221f22",
      "tab.activeBackground": "#2d2a2e",
      "tab.border": "#221f22",
      "tab.hoverBorder": "#2d2a2e",
      "tab.activeBorder": "#2d2a2e",
      "tab.unfocusedActiveBorder": "#2d2a2e",
      "editorLineNumber.activeForeground": "#939293"
    },
    "[Night Owl]": {
      "activityBar.background": "#000C1D",
      "activityBar.border": "#102a44",
      "sideBar.background": "#001122",
      "sideBar.border": "#102a44",
      "sideBar.foreground": "#8BADC1",
      "tab.border": "#010e1a",
      "contrastBorder": "#011627"
    },
    "[Overnight Italics]": {
      "sideBar.background": "#0A1222",
      "tab.border": "#010e1a",
      "contrastBorder": "#011627"
    },
    "[poimandres]": {
      "editorBracketHighlight.foreground1": "#d0679d",
      "editorBracketHighlight.foreground2": "#fffac2",
      "editorBracketHighlight.foreground3": "#5fb3a1",
      "editorBracketHighlight.foreground4": "#89ddff",
      "panel.border": "#1b1e28",
      "input.background": "#1e212b"
    },
    "[poimandres] [Noir Poimandres*]": {
      "statusBarItem.warningBackground": "#fffac2",
      "statusBarItem.warningForeground": "#1b1e28",
      "statusBarItem.warningHoverForeground": "#1b1e28",
      "statusBarItem.errorHoverForeground": "#ffffff",
      "terminal.ansiBrightBlack": "#506477"
    }
  },
  "workbench.sideBar.location": "right",
  "workbench.tips.enabled": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.showIcons": false,
  "breadcrumbs.enabled": false,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": true,
  "window.titleBarStyle": "custom",
  "editor.fontSize": 18,
  "editor.fontFamily": "'Dank Mono', Mononoki, LigaIBMPlexMono, monospace",
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "editor.minimap.enabled": false,
  "editor.cursorStyle": "line-thin",
  "editor.cursorBlinking": "phase",
  "editor.stickyScroll.enabled": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.renderLineHighlight": "all",
  "editor.scrollBeyondLastLine": false,
  "editor.renderWhitespace": "selection",
  "editor.lightbulb.enabled": "off",
  "editor.occurrencesHighlight": "off",
  "editor.linkedEditing": true,
  "editor.rulers": [80],
  "editor.smoothScrolling": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.snippetSuggestions": "top",
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": true
  },
  "editor.guides.indentation": false,
  "editor.gotoLocation.multipleDefinitions": "goto",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "explorer.autoReveal": false,
  "explorer.openEditors.visible": 0,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.profiles.osx": {
    "zsh": {
      "path": "/usr/local/bin/zsh"
    }
  },
  "workbench.fontAliasing": "default",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "'Dank Mono', 'Symbols Nerd Font Mono', monospace",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "window.restoreFullscreen": true,
  "window.title": "${rootPath}${separator}${activeEditorShort}",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "css.validate": false,
  "extensions.ignoreRecommendations": true,
  "telemetry.enableTelemetry": false,
  "telemetry.telemetryLevel": "off",
  "intelephense.telemetry.enabled": false,
  // "eslint.options": {
  //   "ignorePath": ".gitignore"
  // },
  "files.associations": {
    "*.tpl": "php"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "tailwindCSS.includeLanguages": {
    "php": "html"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "php": "html"
  },
  "emmet.syntaxProfiles": {
    "php": "html"
  },
  "monokaiPro.fileIconsMonochrome": true,
  "vscode_custom_css.imports": [
    "file:///Users/fima/.dotfiles/vscode/customizations.css"
  ],
  "vscode_custom_css.statusbar": false,
  "formattingToggle.affects": ["editor.formatOnSave"],
  "search.exclude": {
    "**/*.js.map": true
  },
  "[php]": {
    "editor.defaultFormatter": "vsce-toolroom.vscode-beautify"
  },
  "[javascript][javascriptreact][typescript][typescriptreact]": {
    "editor.formatOnSave": false
  },
  "quokka.showStartViewOnFeatureRelease": false,
  "terminal.integrated.env.osx": {
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.minimumContrastRatio": 1,
  "editor.accessibilitySupport": "off",
  "git.openRepositoryInParentFolders": "never",
  "update.showReleaseNotes": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "workbench.editor.tabActionCloseVisibility": false,
  "jest.runMode": {
    "type": "on-demand"
  },
  "workbench.editor.empty.hint": "hidden",
  "update.mode": "default",
  "totalTypeScript.hideAllTips": true,
  "errorLens.excludeBySource": [
    "eslint(@typescript-eslint/ban-types)",
    "eslint(@typescript-eslint/no-empty-function)",
    "eslint(@typescript-eslint/no-unused-vars)",
    "eslint(prettier/prettier)",
    "eslint(arrow-parens)",
    "eslint(no-console)",
    "eslint(no-empty-pattern)",
    "eslint(no-empty)",
    "eslint(semi)",
    "ts(6133)"
  ],
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "workbench.editor.editorActionsLocation": "titleBar",
  "workbench.layoutControl.enabled": false,
  "workbench.navigationControl.enabled": false,
  "chat.commandCenter.enabled": false,
  "vscode_custom_css.statusbar": false,
  "workbench.activityBar.location": "hidden",
  "inlineChat.holdToSpeech": false
}
