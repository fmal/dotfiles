{
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Monokai Pro",
  "workbench.iconTheme": "Monokai Pro Monochrome Icons",
  "workbench.colorCustomizations": {
    "[Monokai Pro]": {
      "titleBar.activeBackground": "#221f22",
      "editorGroupHeader.tabsBackground": "#221f22",
      "tab.inactiveBackground": "#221f22",
      "tab.activeBackground": "#2d2a2e",
      "tab.border": "#221f22",
      "tab.activeBorder": "#2d2a2e",
      "tab.unfocusedActiveBorder": "#2d2a2e",
      "editorActiveLineNumber.foreground": "#939293"
    }
  },
  "workbench.activityBar.visible": false,
  "workbench.editor.tabCloseButton": "off",
  "workbench.sideBar.location": "right",
  "workbench.statusBar.feedback.visible": false,
  "workbench.tips.enabled": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.showIcons": false,
  "editor.fontSize": 16,
  "editor.fontFamily": "IBM PLex Mono, monospace",
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": false,
  "editor.cursorStyle": "line-thin",
  "editor.cursorBlinking": "phase",
  "editor.renderLineHighlight": "all",
  "editor.scrollBeyondLastLine": false,
  "editor.renderWhitespace": "boundary",
  "editor.lightbulb.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.rulers": [
    80
  ],
  "editor.smoothScrolling": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.snippetSuggestions": "top",
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },
  "explorer.autoReveal": false,
  "explorer.openEditors.visible": 0,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "SF Mono, monospace",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "window.restoreFullscreen": true,
  "window.title": "${rootPath}${separator}${activeEditorShort}",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "javascript.validate.enable": false,
  "css.validate": false,
  "extensions.ignoreRecommendations": true,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "atomKeymap.promptV3Features": true,
  "prettier.disableLanguages": [
    "js"
  ],
  "eslint.autoFixOnSave": true,
  "projectManager.treeview.visible": false,
  "projectManager.showProjectNameInStatusBar": false,
  "monokaiPro.fileIconsMonochrome": true,
  "[javascript]": {
    "editor.formatOnSave": false
  }
}
